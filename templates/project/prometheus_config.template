# Prometheus config generated by Boiler; DO NOT EDIT.
global:
  scrape_interval:     30s
  evaluation_interval: 30s

scrape_configs:
  # Prometheus node job
  - job_name: 'prometheus'
    scrape_interval: 15s
    scrape_timeout: 15s
    static_configs:
      - targets: ['localhost:9090']

  # Boiler app job
  - job_name: 'boiler'
    scrape_interval: 15s
    scrape_timeout: 15s
    static_configs:
      - targets: ['host.docker.internal:8092']
